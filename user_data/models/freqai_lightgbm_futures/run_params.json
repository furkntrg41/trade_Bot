        "data_split_parameters": {
            "test_size": 0.33,
            "random_state": 42,
            "shuffle": false
        },
        "model_training_parameters": {
            "n_estimators": 1000,
            "learning_rate": 0.02,
            "max_depth": 8,
            "num_leaves": 64,
            "min_child_samples": 20,
            "n_jobs": 16,
            "verbosity": -1
        },
        "write_metrics_to_disk": false,
        "conv_width": 1,
        "save_backtest_models": false,
        "activate_tensorboard": true,
        "wait_for_training_iteration_on_reload": true,
        "continual_learning": false,
        "keras": false,
        "override_exchange_check": false
    },
    "timeframe": "5m",
    "stake_amount": "unlimited",
    "stake_currency": "USDT",
    "max_open_trades": 3,
    "pairs": [
        "BTC/USDT:USDT",
        "ETH/USDT:USDT"
    ]
